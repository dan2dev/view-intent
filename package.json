{
  "private": false,
  "name": "view-intent",
  "version": "1.4.0",
  "description": "Framework to create view navigation pattern.",
  "main": "lib/main",
  "types": "lib/main",
  "scripts": {
    "test": "jest --no-cache",
    "dev": "npm-run-all --parallel tsc && exit 0",
    "tsc": "tsc -w  && exit 0",
    "prepublish-": "yarn build && exit 0",
    "build": "yarn clean && tsc && exit 0",
    "clean": "rimraf @dist/* @types/* lib/* && exit 0",
    "clean-node": "rimraf ./yarn.lock ./package-lock.json ./node_modules/* && exit 0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/view-intent/view-intent.git"
  },
  "author": {
    "name": "Dan C Castro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/view-intent/view-intent/issues"
  },
  "homepage": "https://github.com/view-intent/view-intent#readme",
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/jquery": "^3.3.4",
    "@types/jsx-chai": "^3.0.16",
    "@types/lodash": "^4.14.112",
    "jest": "^23.2.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/uniqid": "^4.1.2",
    "abstract-observable": "^0.1.10",
    "ajax-worker": "^1.2.6",
    "class-transformer": "^0.1.9",
    "es6-shim": "^0.35.3",
    "localforage": "^1.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "reflect-metadata": "^0.1.12",
    "uniqid": "^4.1.1",
    "utility-collection": "^2.2.10",
    "view-intent-store": "^1.2.1"
  }
}
