{
  "private": false,
  "name": "view-intent",
  "version": "1.2.9",
  "description": "",
  "main": "lib/main",
  "types": "lib/main",
  "scripts": {
    "test": "jest --no-cache",
    "dev": "npm-run-all --parallel tsc && exit 0",
    "sass": "sass --watch src/main.scss:lib/main.css && exit 0",
    "tsc": "tsc -w",
    "build": "tsc && exit 0",
    "clean": "rimraf lib/* && exit 0",
    "clean-node": "rimraf ./yarn.lock ./package-lock.json ./node_modules/* && exit 0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan2dev/view-intent.git"
  },
  "author": {
    "name": "Dan C Castro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/view-intent/view-intent/issues"
  },
  "homepage": "https://github.com/view-intent/view-intent#readme",
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "jest": "^23.2.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@types/uniqid": "^4.1.2",
    "abstract-observable": "^0.1.2",
    "ajax-worker": "^1.2.6",
    "class-transformer": "^0.1.9",
    "es6-shim": "^0.35.3",
    "localforage": "^1.7.2",
    "reflect-metadata": "^0.1.12",
    "uniqid": "^4.1.1",
    "utility-collection": "^2.2.3"
  }
}
